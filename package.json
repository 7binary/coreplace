{
  "name": "coreplace",
  "version": "1.0.0",
  "description": "",
  "files": [
    "dist"
  ],
  "bin": "dist/index.js",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "dev": "ts-node src/index.ts",
    "run": "node dist/index.js serve",
    "start": "tsc --watch --preserveWatchOutput",
    "prepublishOnly": "esbuild src/index.ts --platform=node --outfile=dist/index.js --bundle --minify --define:process.env.NODE_ENV=\\\"production\\\"",
    "clean": "del ./build/*",
    "build": "yarn clean && tsc && tscpaths -p tsconfig.json -s ./src -o ./build",
    "git": "git add . && git commit -m \"Updates\" --allow-empty && git push",
    "pub": "npm run git && npm version patch && npm run build && npm publish --access public"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/node": "14.14.44",
    "chalk": "4.1.1",
    "commander": "7.2.0",
    "del-cli": "3.0.1",
    "esbuild": "0.8.26",
    "replace-in-file": "6.2.0",
    "ts-node": "9.1.1",
    "typescript": "4.2.4"
  }
}
